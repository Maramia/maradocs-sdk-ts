{
    "name": "@maramia/maradocs-sdk-ts",
    "version": "1.0.0",
    "description": "TypeScript SDK for MaraDocs API (https://maradocs.io)",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./models/*": {
            "types": "./dist/models/*.d.ts",
            "default": "./dist/models/*.js"
        },
        "./shared/*": {
            "types": "./dist/shared/*.d.ts",
            "default": "./dist/shared/*.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "tsc",
        "prepare": "npm run build",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "maradocs",
        "api",
        "sdk",
        "typescript",
        "pdf",
        "email",
        "document-generation"
    ],
    "author": "Maramia GmbH",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/maramia/maradocs-sdk-typescript.git"
    },
    "homepage": "https://maradocs.io",
    "bugs": {
        "url": "https://github.com/maramia/maradocs-sdk-ts/issues"
    },
    "packageManager": "pnpm@10.12.4",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "js-md5": "^0.8.3",
        "libsodium-wrappers": "^0.7.15"
    },
    "peerDependencies": {
        "zod": "^4.0.0"
    },
    "devDependencies": {
        "@types/js-md5": "^0.8.0",
        "@types/libsodium-wrappers": "^0.7.14",
        "@types/node": "^25.0.9",
        "dotenv": "^17.2.3",
        "typescript": "^5.9.3"
    }
}